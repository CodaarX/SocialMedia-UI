<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.UserInterface" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_grey</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="baseLayoutportrait">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="constraintLayout">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:layout_height">124dp</item>
    </style>
    <style name="scrollChildLayout">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingBottom">30dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="scrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="color">@color/black</item>
        <item name="android:layout_marginStart">27dp</item>
        <item name="android:layout_marginEnd">26dp</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="goodMorningTextView">
        <item name="android:clickable">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">149dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="layout_constraintHorizontal_bias">0.0</item>
        <item name="layout_constraintVertical_bias">0.23</item>
    </style>
    <style name="searchView">
        <item name="iconifiedByDefault">false</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="loveImaageView">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">26dp</item>
    </style>
    <style name="menuImageView">
        <item name="tint">@color/text_color</item>
        <item name="android:layout_width">26dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="layout_constraintHorizontal_bias">0.95</item>
        <item name="layout_constraintVertical_bias">0.19</item>
    </style>
    <style name="profilePicture">
        <item name="android:layout_margin">40dp</item>
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_height">113dp</item>
        <item name="layout_constraintVertical_bias">0.676</item>
    </style>
    <style name="userName">
        <item name="android:gravity">center</item>
        <item name="android:textSize">30sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="layout_editor_absoluteY">197dp</item>
    </style>
    <style name="userLocation">
        <item name="android:textAlignment">center</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_margin">16dp</item>
        <item name="android:textSize">16sp</item>
        <item name="layout_constraintHorizontal_bias">1.0</item>
        <item name="layout_editor_absoluteY">257dp</item>
    </style>
    <style name="followButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_margin">16dp</item>
    </style>
    <style name="messageButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_margin">16dp</item>
    </style>
    <style name="gridLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">25dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:columnCount">3</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:useDefaultMargins">true</item>
        <item name="layout_editor_absoluteX">0dp</item>
    </style>
    <style name="gridlinear">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_columnWeight">1</item>
    </style>
    <style name="rectangle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="button3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">16dp</item>
    </style>
    <style name="switch1">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginEnd">6dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:textSize">10sp</item>
    </style>
</resources>